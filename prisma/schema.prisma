

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}


model Customer {
   id                 String  @id @default(cuid())
   first_name         String  
   last_name          String
   address            String
   phone              String
   email              String
   isCanada           String
   prefer_color       String
   lang               String
   linked_in_url      String
   facebook_url       String
   education          Json?
   experience         Json?
   skills             Json?
   interships         Json?
   cources            Json?


   createAt           DateTime  @default(now())
   updatedAt          DateTime?

    @@unique([id])
}