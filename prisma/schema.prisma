

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}


model client {
   id                 String  @id @default(cuid())
   first_name         String  
   last_name          String
   address            String
   phone              String
   email              String
   createAt           DateTime  @default(now())
   updatedAt          DateTime?

    @@unique([id])
}